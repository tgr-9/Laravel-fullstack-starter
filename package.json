{
  "version": "0.0.0",
  "private": true,
  "packageManager": "pnpm@8.5.0",
  "author": "Creasi Developers <developers@creasi.co>",
  "scripts": {
    "build": "vite build",
    "dev": "vite",
    "lint": "eslint --ext .ts,.json,.vue resources",
    "prepare": "husky install scripts/husky"
  },
  "dependencies": {
    "@sentry/browser": "^7.56.0",
    "@sentry/tracing": "^7.56.0",
    "alpinejs": "^3.12.2",
    "axios": "^1.4.0",
    "date-fns": "^2.30.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.6",
    "@commitlint/config-conventional": "^17.6.6",
    "@creasico/eslint-config": "^0.0.1",
    "@fontsource/nunito": "^5.0.3",
    "@types/alpinejs": "^3.7.2",
    "@types/node": "^18.16.18",
    "cross-env": "^7.0.3",
    "eslint": "^8.43.0",
    "husky": "^8.0.3",
    "laravel-vite-plugin": "^0.7.8",
    "lint-staged": "^13.2.2",
    "typescript": "^5.1.3",
    "vite": "~4.3.9",
    "vite-plugin-windicss": "^1.9.0",
    "windicss": "^3.5.6"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{json,js,ts,vue}": [
      "eslint --fix"
    ],
    "{app,config,database,resources,tests}/**/*.php": [
      "php vendor/bin/pint --preset laravel"
    ]
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true,
      "browser": true
    },
    "extends": "@creasico/eslint-config/vue",
    "parserOptions": {
      "tsconfigRootDir": ".",
      "project": "./tsconfig.json"
    }
  }
}
