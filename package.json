    "axios": "^1.2.0",
    "date-fns": "^2.29.3"
  },
  "devDependencies": {
    "@antfu/eslint-config-vue": "^0.27.0",
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@fontsource/nunito": "^4.5.11",
    "@types/alpinejs": "^3.7.1",
    "@types/node": "^18.11.9",
    "cross-env": "^7.0.3",
    "eslint": "^8.28.0",
    "husky": "^8.0.2",
    "laravel-vite-plugin": "^0.6.1",
    "lint-staged": "^13.0.4",
    "typescript": "^4.9.3",
    "vite": "^3.2.4",
    "vite-plugin-windicss": "^1.8.8",
