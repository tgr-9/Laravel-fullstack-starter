{
  "version": "0.0.0",
  "private": true,
  "packageManager": "pnpm@7.5.0",
  "author": "Creasi Developers <developers@creasi.co>",
  "engines": {
    "node": "14.x || 16.x"
  },
  "scripts": {
    "build": "mix --production",
    "dev": "mix",
    "hot": "mix watch --hot",
    "prepare": "husky install scripts/husky",
    "watch": "mix watch",
    "watch:poll": "mix watch -- --watch-options-poll=1000"
  },
  "dependencies": {
    "@sentry/browser": "^7.7.0",
    "@sentry/tracing": "^7.7.0",
    "date-fns": "^2.29.1"
  },
  "devDependencies": {
    "@antfu/eslint-config-vue": "^0.25.2",
    "@babel/core": "^7.18.9",
    "@babel/plugin-proposal-object-rest-spread": "^7.18.9",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.18.9",
    "@babel/preset-env": "^7.18.9",
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@tailwindcss/forms": "^0.5.2",
    "alpinejs": "^3.10.3",
    "autoprefixer": "^10.4.7",
    "axios": "^0.27.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.20.0",
    "husky": "^8.0.1",
    "laravel-mix": "^6.0.49",
    "lint-staged": "^13.0.3",
    "lodash": "^4.17.21",
    "postcss": "^8.4.14",
    "postcss-import": "^14.1.0",
    "tailwindcss": "^3.1.6",
    "typescript": "^4.7.4",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "{resources,scripts}/**/*.{ts,js,css}": [
      "eslint --fix"
    ],
    "{package,tsconfig}.json": [
      "eslint --fix"
    ],
    "*.config.{js,ts}": [
      "eslint --fix"
    ],
    "{app,config,database,resources,tests}/**/*.php": [
      "php vendor/bin/pint --preset laravel"
    ]
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true,
      "browser": true
    },
    "extends": "@antfu/vue",
    "rules": {}
  }
}
